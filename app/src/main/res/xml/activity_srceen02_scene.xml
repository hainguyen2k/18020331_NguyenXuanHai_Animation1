            motion:layout_constraintStart_toStartOf="@+id/imageView15"
            motion:layout_constraintEnd_toEndOf="@+id/imageView15"
            motion:layout_constraintTop_toBottomOf="@+id/imageView15"
            android:layout_marginTop="12dp"
            motion:layout_constraintHorizontal_bias="0.507"
            motion:layout_constraintBottom_toTopOf="@+id/view"
            motion:layout_constraintVertical_bias="0.33" />
